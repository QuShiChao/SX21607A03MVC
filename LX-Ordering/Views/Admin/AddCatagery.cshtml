
@{
    ViewBag.Title = "AddCatagery";
    Layout = "~/Views/AdminPage1.cshtml";
}
<div class="content-wrapper">
    <div class="col-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <h3 class="card-title">添加菜系</h3>
                <p class="card-description">
                    添加菜系
                </p>
                <form class="forms-sample" action="/Admin/AddCatagery" method="post">
                    <div class="form-group">
                        <label for="exampleInputName1">菜系名称：</label>
                        <input type="text" class="form-control" id="exampleInputName1" placeholder="Name" name="Name">
                    </div>
                    <button type="submit" class="btn btn-gradient-primary mr-2">添加</button>
                    <button class="btn btn-light">返回</button>
                </form>
            </div>
        </div>
    </div>
</div>

